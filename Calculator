#include <iostream>

using namespace std;

int main()
{
    int a,b;
    char op;
    
    cout<<"enter two numbers:";
    cin>>a>>b;
    
    cout<<"enter operand:";
    cin>>op;
    
    switch (op)
    {
        case '+':
        cout<<a+b<<endl;
        break;
        
        case '-':
        cout<<a-b<<endl;
        break;
        
        case '*':
        cout<<a*b<<endl;
        break;
        
        case '/':
        cout<<a/b<<endl;
        break;
        
        default:
        cout<<"operator not found!"<<endl;
        break;
    }

    return 0;
}
